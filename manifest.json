{
    "action": {
      "default_popup": "popup.html"
    },
    
    "background": {
      "service_worker": "background.js"
    },
    "description": "auto write",
    "host_permissions": [
      "https://s0urce.io/*",
      "http://localhost/*",
      "https://google.com/*"
    ],
    "manifest_version": 3,
    "name": "s0urce.io BOT",
    "permissions": [
      "webRequest",
      "tabs",
      "storage",
      "activeTab",
      "scripting",
      "clipboardRead",
      "clipboardWrite",
      "scripting",
      "downloads"
  
    ],
    "version": "0.0.1",
    "web_accessible_resources": [
      {
        "resources": ["dbtest.json"],
        "matches": ["<all_urls>"]
      }
    ],
    "content_scripts": [
      {
        "matches": ["https://s0urce.io/*, http://localhost/*, https://google.com/*"],
        "js": ["injected.js"]
      }]
  } 
   
  
  
  
  
  
  
  